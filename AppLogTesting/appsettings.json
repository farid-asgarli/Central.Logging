{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "Extended": {
      "MinimumLevel": "Debug",
      "ApplicationName": "MyApplication",
      "Environment": "Development",
      "IncludeScopes": true,
      "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] [{Category}] {SourceContext}: {Message:lj}{NewLine}{Exception}",
      "EnrichmentProperties": {
        "Version": "1.0.0",
        "Component": "WebApi"
      }
    }
  }
}
